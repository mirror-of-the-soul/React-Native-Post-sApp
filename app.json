{
  "expo": {
    "name": "Self Posts App",
    "slug": "rn-self-posts",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/rn-splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#242832"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "ru.evgdev.rnselfapp",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "ru.evgdev.rnselfapp",
      "versionCode": 1,
      "permissions": [
        "CAMERA",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#242832"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
